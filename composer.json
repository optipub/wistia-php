{
    "name": "optipub/wistia-php",
    "description": "PHP wrapper for Wistia API",
    "keywords": ["wistia","php","client","wrapper","video"],
    "homepage": "http://github.com/optipub/wistia-php",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "OptiPub",
            "email": "dev@optipub.com",
            "homepage": "http://opti.pub"
        },
        {
            "name": "Automattic",
            "email": "info@automattic.com",
            "homepage": "https://automattic.com"
        },
        {
            "name": "Nicola Mustone",
            "email": "nicola@automattic.com",
            "homepage": "https://nicola.blog",
            "role": "Developer"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^7"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*"
    },
    "autoload": {
        "psr-4": {
            "Automattic\\Wistia\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Automattic\\Wistia\\Tests\\": "tests/"
        },
        "classmap": [
            "src/",
            "tests/"
        ]
    }
}
